{
	"name": "realmexchange",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --mode dev",
		"setup": "npm run db:seed:wrangler && npm run r2:putobjectsxml && cp .env.dev .env.local",
		"build": "vite build",
		"preview": "npm run build && wrangler pages dev --inspector-port=1234 --persist-to=../.wrangler",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit generate && wrangler d1 migrations apply realm-exchange --local --persist-to=../.wrangler",
		"db:migrate:remote": "npm run db:migrate && wrangler d1 migrations apply realm-exchange --remote",
		"db:studio": "drizzle-kit studio",
		"db:seed:wrangler": "chmod +x ./test/seed-wrangler.sh && ./test/seed-wrangler.sh",
		"db:clear:accounts": "sqlite3 ../.wrangler/v3/d1/miniflare-D1DatabaseObject/fa92c093e24cb65a04d2fe43a61e384cab5180d69d421c0a3c38c0f3151adeda.sqlite \"DELETE FROM account;\"",
		"db:clear:accounts:remote": "wrangler d1 execute realm-exchange --remote --command \"DELETE FROM account;\"",
		"db:clear:users": "sqlite3 ../.wrangler/v3/d1/miniflare-D1DatabaseObject/fa92c093e24cb65a04d2fe43a61e384cab5180d69d421c0a3c38c0f3151adeda.sqlite \"DELETE FROM user;\"",
		"db:clear:users:remote": "wrangler d1 execute realm-exchange --remote --command \"DELETE FROM user;\"",
		"r2:putobjectsxml": "wrangler r2 object put realm-exchange/Objects.xml --file ../Objects.xml --local --persist-to=../.wrangler",
		"deploy": "npm run build && wrangler pages deploy",
		"cf-typegen": "wrangler types src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.0",
		"@libsql/client": "^0.15.15",
		"@lucide/svelte": "^0.544.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/jwt": "^0.3.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/vite": "^4.1.13",
		"@types/jsdom": "^27.0.0",
		"@types/node": "^22.19.0",
		"bits-ui": "^2.14.3",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.5",
		"drizzle-seed": "^0.3.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"wrangler": "^4.46.0"
	},
	"dependencies": {
		"fast-xml-parser": "^5.3.1",
		"jsdom": "^27.2.0",
		"saxes": "^6.0.0"
	}
}
